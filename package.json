{
  "name": "vl-ui-upload",
  "version": "0.0.2",
  "description": "Gebruik de upload component om één of meerdere bestanden te selecteren of te slepen naar het upload veld. De gebruiker kan alternatief ook één of meerdere bestanden uploaden door op de link in het upload veld te klikken en de bestanden te selecteren in het Bestand menu.",
  "main": "vl-upload.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "test-debug": "wct -l chrome,firefox --npm -p",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-upload.js -- *.src.js",
    "release:prepare:imports": "npm run release:prepare:imports:core && npm run release:prepare:imports:dist",
    "release:prepare:imports:core": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-upload.js",
    "release:prepare:imports:dist": "replace '../dist/upload.js' '/node_modules/vl-ui-upload/dist/upload.js' vl-upload.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-upload/style.css' vl-upload.js",
    "release:prepare:commit": "git add -f vl-upload.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-upload.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-upload/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-upload#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "dependencies": {
    "vl-ui-core": "1.4.2",
    "vl-ui-link": "1.1.1",
    "vl-ui-icon": "2.0.4"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2",
    "vl-ui-link": "^1.1.1",
    "vl-ui-icon": "^2.0.4"
  },
  "devDependencies": {
    "@govflanders/vl-ui-core": "3.7.2",
    "@govflanders/vl-ui-util": "3.7.2",
    "@govflanders/vl-ui-upload": "3.7.2",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2",
    "replace": "1.0.0",
    "np": "3.0.4",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "http-server": "0.11.1",
    "sinon": "7.3.1",
    "vl-ui-util": "1.0.6"
  }
}
